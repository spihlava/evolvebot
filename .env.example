# Google Gemini Settings
GEMINI_API_KEY=your_gemini_key_here
GEMINI_MODEL=gemini-2.5-flash

# Minimax Settings (Anthropic-compatible)
MINIMAX_API_KEY=your_minimax_key_here
MINIMAX_API_BASE=https://api.minimax.io/anthropic
MINIMAX_MODEL=MiniMax-M2.5

# Provider: "minimax" | "gemini" | "ollama"
EVOLVE_PROVIDER=minimax
EXECUTOR_MODEL=MiniMax-M2.5

# Ollama settings (only needed if EVOLVE_PROVIDER=ollama)
OPENAI_BASE_URL=http://localhost:11434/v1
OPENAI_API_KEY=ollama

# Execution Mode: direct | chroot | docker
EXEC_MODE=direct
